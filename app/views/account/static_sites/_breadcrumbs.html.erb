<% static_site ||= @static_site %>
<% team ||= @team || static_site&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :static_sites] %>
<% if static_site&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: static_site.label_string, url: [:account, static_site] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'static_sites' %>
